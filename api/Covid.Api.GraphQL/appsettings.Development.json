{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Debug",
        "Microsoft": "Debug",
        "Microsoft.AspNetCore": "Debug",
        "Microsoft.EntityFrameworkCore.Database.Command": "Debug",
        "CorrelationId.CorrelationIdMiddleware": "Debug",
        "MongoDB.Driver.IMongoDatabase": "Debug"
      }
    }
  },
  "FeatureManagement": {
    "Caching": false
  },
  "EntityFramework": {
      "EnableDetailedErrors": true,
      "EnableSensitiveDataLogging": true
    },
    "GraphQL": {
      "ExposeExceptions": true
    }
}