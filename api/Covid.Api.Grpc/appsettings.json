{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "TimeseriesDatabase": {
    "ConnectionString": ""
  },
  "EntityFramework": {
    "MigrationAssembly": "Covid.Api.Grpc",
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": true
  },
}
