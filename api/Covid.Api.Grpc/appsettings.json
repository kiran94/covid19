{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Debug",
        "Microsoft": "Debug",
        "Microsoft.AspNetCore": "Debug",
        "Microsoft.EntityFrameworkCore.Database.Command": "Debug",
        "CorrelationId.CorrelationIdMiddleware": "Debug"
      }
    }
  },
  "ElasticSearch": {
    "Url": ""
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "TimeseriesDatabase": {
    "ConnectionString": ""
  },
  "EntityFramework": {
    "MigrationAssembly": "Covid.Api.Grpc",
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": true
  },
}
